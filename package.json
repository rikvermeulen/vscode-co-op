    "typescript": "^4.9.5",
    "ts-loader": "^9.4.2",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "@vscode/test-electron": "^2.3.0",
    "tsup": "^6.6.3",
